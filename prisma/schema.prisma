generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Mvp {
  id           Int      @id @default(autoincrement())
  name         String
  lastKillTime DateTime @default(now())
  respawnTime  Int @default(9000000)
  isAlive      Boolean  @default(true)
  killerName   String? @default("")
  img          String?
}
