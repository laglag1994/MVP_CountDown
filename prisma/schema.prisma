generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model MVP {
  id           Int      @id @default(autoincrement())
  name         String
  lastKillTime DateTime @default(now())
  respawnTime  DateTime
  mvpStatus    Boolean
  img          String?
}
